// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider   = "prisma-client-js"
  output     = "node_modules/.prisma/client"
  engineType = "binary"
}

model Task {
  id           String    @id @default(cuid())
  label        String    @default("")
  priority     String?
  isComplete   Boolean   @default(false)
  assignedTo   Person?   @relation("Task_assignedTo", fields: [assignedToId], references: [id])
  assignedToId String?   @map("assignedTo")
  finishBy     DateTime?

  @@index([assignedToId])
}

model Person {
  id       String  @id @default(cuid())
  name     String  @default("")
  email    String  @unique @default("")
  password String?
  tasks    Task[]  @relation("Task_assignedTo")
}

model Listing {
  id      String  @id @default(cuid())
  title   String  @default("")
  email   String  @unique @default("")
  status  String?
  address String  @default("")
  lng     String  @default("")
  lat     String  @default("")
  agent   Agent?  @relation("Agent_listings")
}

model Agent {
  id         String   @id @default(cuid())
  title      String   @default("")
  email      String   @unique @default("")
  address    String   @default("")
  lng        String   @default("")
  lat        String   @default("")
  listings   Listing? @relation("Agent_listings", fields: [listingsId], references: [id])
  listingsId String?  @unique @map("listings")
}